#1장_연습문제4
import turtle
turtle.shape('turtle')
turtle.forward(100)
turtle.left(90)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)  
turtle.right(90)
turtle.forward(100)
turtle.left(90)
turtle.forward(100)